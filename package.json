{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "postinstall": "yarn --cwd src/shared/ install --pure-lockfile --ignore-scripts",
    "start": "REACT_DEBUGGER='unset ELECTRON_RUN_AS_NODE && open -g rndebugger://set-debugger-loc?port=19001 ||' expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest --watchAll=true",
    "e2e": "concurrently -k --success first 'wait-port -t 60000 19000 && detox test' 'expo start >/dev/null'",
    "typescript": "tsc --noEmit",
    "build:ios:prod": "expo build:ios --release-channel='production'",
    "build:ios:beta": "expo build:ios --release-channel='default'",
    "build:android:prod": "expo build:android --release-channel='production' -t app-bundle",
    "build:android:beta": "expo build:android --release-channel='default' -t app-bundle",
    "ota:prod": "expo publish --release-channel='production' --non-interactive ",
    "ota:beta": "expo publish --release-channel='default' --non-interactive",
    "coverage": "jest --watchAll=false --coverage",
    "lint": "yarn eslint src/**/*.{ts,tsx}"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "yarn lint && yarn typescript && yarn coverage"
    }
  },
  "dependencies": {
    "@contentful/rich-text-html-renderer": "14.1.1",
    "@expo/vector-icons": "^10.0.0",
    "@react-native-community/async-storage": "~1.11.0",
    "@react-native-community/masked-view": "0.1.10",
    "@react-native-community/netinfo": "5.9.2",
    "@react-navigation/bottom-tabs": "5.7.1",
    "@react-navigation/core": "5.12.0",
    "@react-navigation/drawer": "5.8.5",
    "@react-navigation/native": "5.7.0",
    "@react-navigation/routers": "5.4.9",
    "@react-navigation/stack": "5.7.0",
    "contentful": "7.14.5",
    "dayjs": "1.8.29",
    "expo": "^38.0.0",
    "expo-analytics-amplitude": "~8.2.1",
    "expo-blur": "~8.1.2",
    "expo-constants": "~9.1.1",
    "expo-haptics": "~8.2.1",
    "expo-linear-gradient": "~8.2.1",
    "expo-linking": "^1.0.3",
    "expo-permissions": "~9.0.1",
    "expo-screen-orientation": "~1.1.1",
    "expo-splash-screen": "~0.3.1",
    "expo-updates": "~0.2.8",
    "expo-web-browser": "~8.3.1",
    "react": "16.11.0",
    "react-context-theming": "^1.0.0-alpha.7",
    "react-dom": "16.11.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-38.0.2.tar.gz",
    "react-native-appearance": "~0.3.3",
    "react-native-calendars": "1.300.0",
    "react-native-gesture-handler": "~1.6.0",
    "react-native-htmlview": "^0.15.0",
    "react-native-open-notification": "0.1.4",
    "react-native-paper": "3.10.1",
    "react-native-reanimated": "~1.9.0",
    "react-native-safe-area-context": "3.0.2",
    "react-native-screens": "~2.9.0",
    "react-native-shared-element": "0.7.0",
    "react-native-svg": "12.1.0",
    "react-native-web": "~0.11.7",
    "react-navigation-shared-element": "^5.0.0-alpha1",
    "react-redux": "7.2.0",
    "redux": "^4.0.4",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0",
    "sentry-expo": "2.0.3"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "9.1.1",
    "@expo/config": "^3.2.1",
    "@types/detox": "16.4.1",
    "@types/jest": "25.2.1",
    "@types/react": "~16.9.41",
    "@types/react-native": "~0.62.13",
    "@types/react-native-calendars": "1.20.8",
    "@types/react-native-htmlview": "^0.12.2",
    "@types/react-redux": "7.1.9",
    "@types/react-test-renderer": "^16.9.2",
    "@types/redux-mock-store": "^1.0.2",
    "@types/redux-persist": "^4.3.1",
    "@typescript-eslint/eslint-plugin": "3.6.1",
    "@typescript-eslint/parser": "3.6.1",
    "commitlint": "9.1.0",
    "concurrently": "5.2.0",
    "detox": "16.8.2",
    "detox-expo-helpers": "^0.6.0",
    "dotenv": "^8.2.0",
    "eslint": "7.4.0",
    "eslint-plugin-react-hooks": "4.0.8",
    "expo-cli": "3.22.3",
    "expo-detox-cli": "^1.0.10",
    "expo-detox-hook": "^1.0.10",
    "husky": "4.2.5",
    "jest-expo": "^38.0.0",
    "metro": "^0.59.0",
    "metro-core": "^0.59.0",
    "netlify-cli": "2.58.0",
    "react-test-renderer": "16.9.0",
    "redux-mock-store": "^1.5.4",
    "typescript": "~3.9.5",
    "wait-port": "0.2.9"
  },
  "private": true,
  "detox": {
    "test-runner": "jest",
    "configurations": {
      "ios.sim": {
        "binaryPath": "e2e/Exponent.app",
        "type": "ios.simulator",
        "name": "iPhone 11 Pro"
      }
    }
  }
}
