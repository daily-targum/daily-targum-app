{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "postinstall": "yarn --cwd src/shared/ install --pure-lockfile --ignore-scripts",
    "start": "REACT_DEBUGGER='unset ELECTRON_RUN_AS_NODE && open -g rndebugger://set-debugger-loc?port=19001 ||' expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest --watchAll=true --setupFiles ./dotenv.js",
    "e2e": "concurrently -k --success first 'wait-port -t 60000 19000 && detox test' 'expo start >/dev/null'",
    "typescript": "tsc --noEmit",
    "build:ios:prod": "expo build:ios --release-channel='production'",
    "build:ios:beta": "expo build:ios --release-channel='default'",
    "build:android:prod": "expo build:android --release-channel='production' -t app-bundle",
    "build:android:beta": "expo build:android --release-channel='default' -t app-bundle",
    "ota:prod": "expo publish --release-channel='production' --non-interactive ",
    "ota:beta": "expo publish --release-channel='default' --non-interactive",
    "coverage": "jest --watchAll=false --coverage  --setupFiles ./dotenv.js",
    "lint": "yarn eslint src/**/*.{ts,tsx}"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "yarn lint && yarn typescript && yarn coverage"
    }
  },
  "dependencies": {
    "@contentful/rich-text-html-renderer": "^13.4.0",
    "@expo/vector-icons": "^10.0.0",
    "@react-native-community/masked-view": "0.1.6",
    "@react-native-community/netinfo": "5.5.1",
    "@react-navigation/bottom-tabs": "5.4.7",
    "@react-navigation/core": "5.8.1",
    "@react-navigation/drawer": "5.7.7",
    "@react-navigation/native": "5.4.2",
    "@react-navigation/routers": "5.4.6",
    "@react-navigation/stack": "5.3.9",
    "contentful": "7.14.4",
    "dayjs": "1.8.27",
    "expo": "37.0.12",
    "expo-analytics-amplitude": "~8.1.0",
    "expo-blur": "~8.1.0",
    "expo-constants": "~9.0.0",
    "expo-haptics": "~8.1.0",
    "expo-linear-gradient": "~8.1.0",
    "expo-permissions": "~8.1.0",
    "expo-screen-orientation": "~1.0.0",
    "expo-updates": "0.2.1",
    "expo-web-browser": "~8.2.1",
    "react": "16.9.0",
    "react-context-theming": "^1.0.0-alpha.7",
    "react-dom": "16.9.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-37.0.1.tar.gz",
    "react-native-appearance": "~0.3.3",
    "react-native-calendars": "1.265.0",
    "react-native-gesture-handler": "~1.6.0",
    "react-native-htmlview": "^0.15.0",
    "react-native-open-notification": "0.1.4",
    "react-native-paper": "3.10.1",
    "react-native-reanimated": "~1.7.0",
    "react-native-safe-area-context": "0.7.3",
    "react-native-screens": "~2.2.0",
    "react-native-svg": "11.0.1",
    "react-native-web": "^0.11.7",
    "react-native-xml2js": "^1.0.3",
    "react-redux": "7.2.0",
    "redux": "^4.0.4",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0",
    "sentry-expo": "2.0.3"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^8.3.4",
    "@expo/config": "^3.2.1",
    "@types/detox": "16.4.1",
    "@types/jest": "25.2.1",
    "@types/react": "^16.9.11",
    "@types/react-native": "^0.60.22",
    "@types/react-native-calendars": "^1.20.7",
    "@types/react-native-htmlview": "^0.12.2",
    "@types/react-redux": "7.1.9",
    "@types/react-test-renderer": "^16.9.2",
    "@types/redux-mock-store": "^1.0.2",
    "@types/redux-persist": "^4.3.1",
    "@typescript-eslint/eslint-plugin": "2.34.0",
    "@typescript-eslint/parser": "2.34.0",
    "commitlint": "^8.3.5",
    "concurrently": "5.2.0",
    "detox": "16.5.1",
    "detox-expo-helpers": "^0.6.0",
    "dotenv": "^8.2.0",
    "eslint": "7.0.0",
    "eslint-plugin-react-hooks": "^4.0.2",
    "expo-cli": "^3.20.9",
    "expo-detox-cli": "^1.0.10",
    "expo-detox-hook": "^1.0.10",
    "husky": "4.2.5",
    "jest-expo": "^37.0.0",
    "netlify-cli": "2.51.0",
    "react-test-renderer": "16.9.0",
    "redux-mock-store": "^1.5.4",
    "typescript": "3.9.3",
    "wait-port": "^0.2.7"
  },
  "private": true,
  "detox": {
    "test-runner": "jest",
    "configurations": {
      "ios.sim": {
        "binaryPath": "e2e/Exponent.app",
        "type": "ios.simulator",
        "name": "iPhone 11 Pro"
      }
    }
  }
}
